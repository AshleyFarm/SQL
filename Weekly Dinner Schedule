SELECT * FROM dinners;

-- To find easy dinners
SELECT name, difficulty FROM dinners WHERE difficulty = "Easy";

-- Pick 6 random dinners for each week, Sunday is leftovers/ order in day
SELECT name, cuisine, difficulty FROM dinners
ORDER BY RANDOM()
LIMIT 6;
